{
    "name": "webpress/interaction",
    "description": "",
    "authors": [{
        "name": "buiduy",
        "email": "duybv@vicoders.com"
    }],
    "autoload": {
        "classmap": [],
        "psr-4": {
            "VCComponent\\Laravel\\Interaction\\": "src/app"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Package\\Test\\": "tests/"
        }
    },
    "require": {
        "webpress/core": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "orchestra/testbench": "^4.3",
        "spatie/phpunit-watcher": "^1.21"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "VCComponent\\Laravel\\Interaction\\Providers\\InteractionServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit --color=always"
        ],
        "test:watch": [
            "vendor/bin/phpunit-watcher watch"
        ],
        "coverage": [
            "vendor/bin/phpunit --coverage-html coverages"
        ]
    }
}